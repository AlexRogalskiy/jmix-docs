<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://jmix.io/schema/ui/window"
        xmlns:maps="http://jmix.io/schema/maps/ui"
        caption="msg://caption"
        messagesPack="com.haulmont.demo.maps.web.order">
    <data readOnly="true">
        <collection id="ordersDc"
                    class="maps.ex1.entity.Order">
            <loader id="ordersDl">
                <query>
                    <![CDATA[select e from mapst_Order e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="800"
                width="1200"/>
    <layout expand="map">
        <maps:geoMap id="map" width="100%" zoom="5" center="-99.755859, 39.164141">
            <maps:layers selectedLayer="orders">
                <maps:tile id="tiles" tileProvider="sample_CartoTileProvider"/>
                <maps:vector id="orders" dataContainer="ordersDc">
                    <maps:cluster/>
                </maps:vector>
            </maps:layers>
        </maps:geoMap>
    </layout>
</window>